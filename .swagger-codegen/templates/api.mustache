function convertDateToFullDateStringFormat(date: Date | string): string {
  // Convert to RFC 3339 "full-date" format, like "2017-11-27"
  if (date instanceof Date) {
    return date.toISOString().substring(0, 10);
  } else {
    return date;
  }
}

